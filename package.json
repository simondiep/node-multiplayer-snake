{
  "name": "node-multiplayer-snake",
  "version": "1.0.3",
  "description": "A multiplayer snake game built on NodeJs, Express, socket.io, JavaScript ES6, and jspm.",
  "repository": {
    "type": "git",
    "url": "https://github.com/simondiep/node-multiplayer-snake.git"
  },
  "scripts": {
    "postinstall": "jspm install && npm run build",
    "start": "node app.js",
    "pretest": "eslint .",
    "test": "mocha",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha",
    "build": "jspm bundle main --inject"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "socket.io-client": "^1.4.5",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "jspm": "^0.16.30",
    "less-middleware": "^2.1.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.4.5",
    "validator": "^5.1.0"
  },
  "license": "MIT",
  "jspm": {
    "directories": {
      "baseURL": "public/js"
    },
    "dependencies": {
      "socket.io-client": "npm:socket.io-client@^1.4.5"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
